<!--index.wxml-->
<view class="container">
	<!--header-banner  -->
	<view class='header-banner'>
		<view class='head-img'>
			<image bindtap='previewImages' id='ImgHeadPath' binderror='imageError' src='{{ ImgHeadPath }}'></image>
			<text>{{ agentData.userName}}</text>
		</view>
		<view class='head-bg'>
			<image src='http://www.sinelinked.com/static/other/head-bg.jpg'></image>
		</view>
		<!--奖牌  -->
		<view class='medal'>
			<image src='http://www.sinelinked.com/static/other/medal.png'></image>
		</view>
		<!--所属团队  -->
		<!-- <navigator url="/pages/pro-team/index/index?teamId={{ team_id }}" hover-class="navigator-hover"> -->
		<view bindtap='navigatorTeam' class='belongTeam'>
			<image src='http://www.sinelinked.com/static/other/team.png'></image>
			<text>所属团队</text>
		</view>
		<!-- </navigator> -->
	</view>
	
	 <!--所加入团队弹框 -->
	 <view class="team-pop" wx:if="{{ teamPop }}">
		 <view class="team-pop-center">
			 <view class="cell {{ index == chooseActive?'active':'' }}" wx:for="{{joinTeams}}" data-userId = "{{ item.userId }}" data-index = "{{ index }}" bindtap="chooseTeam" >{{ item.userName }}</view>
		 </view>
	 </view>

	<!--main-info  -->
	<view class='main-info'>
		<view class='center'>
			<!-- <view main-info-cell>
        <text>服务范围：</text>
        <text class='cell-val'>{{ city }}</text>
      </view> -->
			<view main-info-cell>
				<text>职业证书编号：</text>
				<text class='cell-val'>{{ agentData.certificateNo || '无'}} </text>
			</view>
			<view main-info-cell>
				<text>从属公司：</text>
				<text class='cell-val'>{{ agentData.companyName || '无' }}</text>
			</view>
			<view main-info-cell>
				<text>险种：</text>
				<text class='cell-val'>{{ agentData.intype || '无' }} </text>
			</view>
			<view main-info-cell>
				<text>执业区域：</text>
				<text class='cell-val'>{{ getCityResultstr }} </text>
			</view>
			<view main-info-cell>
				<text>工号：</text>
				<text class='cell-val'>{{ agentData.jobNumber || '无'}} </text>
			</view>
			<view main-info-cell bindtap='makePhoneCall' class='main-info-cell-warp'>
				<image class='main-info-cell' src='http://www.sinelinked.com/static/other/tel.png'></image>
				<text>联系我</text>
			</view>
		</view>
	</view>

	<view class='main-pro'>
		<view class='center'>
			<text>个人简介</text>
			<view class='main-pro-con'>
				{{ agentData.personSummary || '无' }}
			</view>
		</view>
	</view>

