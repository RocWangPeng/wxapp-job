<!--pages/member/member.wxml-->
<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">


	<swiper-item wx:for="{{ memList }}">
		<view id='swiper'>
			<navigator open-type="redirect" url="/pages/agent/index/index?account_id={{ item.userId }}" hover-class="navigator-hover">
				<image src="{{ item.headImg || 'http://www.sinelinked.com/static/other/head01.jpg' }}" class="slide-image"
				/>
				<view class='name'>{{ item.userName }}</view>
				<view class='bianhao'>{{ item.certificateNo || '无' }}</view>
				<view class='gongsi'>{{ item.companyName  || '无' }}</view>
				<!-- <view class='zhiwei'>{{ item.userName }}</view> -->
				<view bindtap='makePhoneCall' class='phone'>
					<image src='http://www.sinelinked.com/static/other/tel.png'></image>{{ item.phone }}</view>
				<text class='jieshao'>{{ item.personSummary || '保险传递着爱与责任，用专业中立的态度用心为您规划保险，是我们的爱和责任，保险让生活更美好！'  }}</text>
			</navigator>
		</view>
	</swiper-item>

