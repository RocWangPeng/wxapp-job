<navigator wx:if="{{ isStarAgent }}" open-type="navigate" app-id="wx0ff73d8248085825" path="/pages/index/index?userId={{ starAgent.userId }}"
 hover-class="navigator-hover" target="miniProgram">
	<view class="start-main">
		<view class="start-center">
			<view class="start-head-img">
				<image src='{{ starAgent.headImg }}' ></image>
				<image class="gj" src='../../img/gj.png' mode="widthFix"></image>
			</view>
			<view class="start-info">
				<text class="userName">{{ starAgent.userName }}</text>
				<text class="tdzx">团队之星</text>
				<view class="personSummary">{{ starAgent.personSummary || '保险传递着爱与责任，用专业中立的态度用心为您规划保险，是我们的爱和责任，保险让生活更美好！' }}</view>
			</view>
		</view>
	</view>
</navigator>
