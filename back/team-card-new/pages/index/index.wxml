<!--pages/agent/index/index.wxml-->
<view class="top_bg_wap">
	<view>
		<view class="head_img">
			<image src="{{ agentData.headImg }}" ></image>
		</view>
	</view>
	<!-- 所属团队icon -->
	<view class="belongTeam" bindtap='toMember'>
		<image src="http://ii.sinelinked.com/miniProgramAssets/team_icon.png" mode="scaleToFill"></image>
		<text>团队成员</text>
	</view>
</view>
<!-- main -->
<view class="main">
	<view class="content">
		<text class="name">{{ agentData.userName || '无' }}</text>
		<view class="telIconBox" bindtap='makePhoneCall'>
			<image class="telIcon" src="http://ii.sinelinked.com/miniProgramAssets/tel_icon.png" mode="aspectFit"></image>
		</view>


		<view class="cell">
			<text class="cell_label">业务范围</text>
			<text class="cell_value">{{ getCityResultstr || '无'  }}</text>
		</view>

		<view class="cell">
			<text class="cell_label">成立时间</text>
			<text class="cell_value">{{ createTime || '无'  }}</text>
		</view>
		<!-- introduce -->
		<view class="introduce">
		<view class="introduceCon"> {{ agentData.teamSummary || '无' }}</view>
		</view>
		<view class="contact_btn_wap" bindtap='makePhoneCall'>
			<image src="http://ii.sinelinked.com/miniProgramAssets/tel_icon.png" mode="aspectFit"></image>
			<text>联系我</text>
		</view>
	</view>
</view>
