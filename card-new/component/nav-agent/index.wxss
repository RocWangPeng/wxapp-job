.nav-agent{
  width: 100%;
  height: 100vh;
  border-radius: 18rpx;
  z-index: 4;
  display: flex;
  position: absolute;
  top: 0;
  align-items: center;
  justify-content: center;
}
.nav-index{
  z-index: 100;
}
.nav-btn{
  width: 100rpx;
  height: 100rpx;
  z-index: 100;
  text-align: center;
  border-radius: 18rpx;
}
.menu-main{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  border-radius: 14rpx;
  z-index: 1000;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #6197ec;
  pointer-events: all
}
.menu-main .menu-main-icon{
  width: 44rpx;
  height: 44rpx;
  position: relative;
  z-index: 1000;
}
.nav-agent .nav-item-close .nav-item{
  right: 0 !important;
  top: 0 !important;
  bottom: 0;
  left: 0;
  /* opacity: 0; */
}
.menu-box{
  width: 100%;
  height: 100%;
  position: relative;
}
.nav-item{
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  background: #6197ec;
  text-align: center;
  line-height: 76rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  z-index: 20;
  opacity: .9;
  /* right: 0; */
  top: 0;
  transition: .26s ease-in-out;
}
.nav-item-open .nav-item:nth-child(1){
  top: -130rpx;
  right: 0;
}
.nav-item-open .nav-item:nth-child(2){
  top: -92rpx;
  left: -90rpx;
}
.nav-item-open .nav-item:nth-child(3){
  top: 12rpx;
  left: -128rpx;
} 
.nav-item-open .nav-item:nth-child(4){
  top: 102rpx;
  left: -92rpx;
} 
.nav-item-open .nav-item:nth-child(5){
  top: 150rpx;
  right: 0;
} 
.navIcon{
  width: 60%;
  height: 52%
}