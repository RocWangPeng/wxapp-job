page{
	height: 100%;
	background: #e3e4e9 url('http://ii.sinelinked.com/miniProgramAssets/winter-bg.jpg') no-repeat center center;
	background-size: 100% 100%;
}
.flower{
	width: 710rpx;
	height: 241rpx;
	margin-top: 60rpx;
	margin-bottom: 50rpx;
  display: block;
  margin: 60rpx auto 50rpx auto;
}
.search-bar{
	/* width: 680rpx; */
	margin: 0 auto;
	height: 78rpx;
	line-height: 78rpx;
	display: flex;
  justify-content: center
}
.input-warp{
	width: 434rpx;
	height: 78rpx;
	line-height: 78rpx;
	background: #f6f6f6;
	border: 1rpx solid #eeeeee;
	border-radius: 5rpx;
	padding-left: 12rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
}
.input-warp .input{
	display: inline-block;
	line-height: 78rpx;
	height: 78rpx;
	font-size: 24rpx;
	color: #bebebe;
	width: 340rpx;
	padding-left: 20rpx;
	
}
.button-warp{
	display: flex;
}
.btn{
	width: 116rpx;
	height: 78rpx;
	background: #5994f1;
	font-size: 24rpx;
	color: #fff;
	text-align: center;
	border-radius: 6rpx;
}
.agent-btn{
	margin-right: 4rpx;
}
.search-result{
	text-align: center;
	margin-top: 40rpx;
	margin-bottom: 40rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}
.search-result{
  min-height: 120rpx;
}
.search-result text{
	font-size: 24rpx;
	color: #949494;
	padding-left: 12rpx;
   vertical-align: middle
}
.result-warp{
	padding-right: 66rpx;
}
.tip-icon{
  width: 36rpx;
  height: 32rpx;
  vertical-align: middle
}
.card-main{
	width: 500rpx;
	margin: 0 auto;
}
.card-item{
	border-radius: 16rpx;
	width: 294rpx;
	height: 153rpx;
	background: #fff;
	box-shadow: 0 0 22rpx rgba(0,0,0,.2);
	display: flex;
	justify-content: space-between;
	align-items: center;
	box-sizing: border-box;
	padding-left: 35rpx;
	padding-right: 32rpx;
}
.card-item text{
	font-size: 28rpx;
	color: #555;
}
.agent-card{
	padding-left: 66rpx;
}
.agent-card image{
	width: 60rpx;
	height: 100%;
}
.team-card image{
	width: 106rpx;
	height: 100%;
}
.qr-code{
	width: 120rpx;
	height: 120rpx;
}
.card-flex{
	display: flex;
	justify-content: space-between;
	align-items: center;
	position: relative;
	box-sizing: border-box;
}
.card-flex:first-child{
	margin-bottom: 43rpx;
}
.card-flex .getUserInfo{
	position: absolute;
	width: 100%;
	height: 100%;
	opacity: 0;
}
.teamListItem {
  height: 44px;
  line-height: 44px;
  color: #fff !important;
  background: #fff !important;
  color: #495060 !important;
  padding-bottom: 4rpx;
}
.previewImageTeam{
  position: fixed;
  width: 100%;
  height: 100vh;
  top: 0;
  left: 0;
  background: rgba(255,255,255,.6)
}
.previewImageTeam>view{
  width: 100%;
  height:100%;
  position: relative;
}
.previewImageTeam>view image{
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  margin:auto;
}
.previewImageTeamCode{
  width: 400rpx;
  height: 400rpx;
}
.previewImageTeam .qr-code-tip{
  text-align: center;
  position: absolute;
  top: 40%;
  bottom: 0;
  right: 0;
  left: 0;
  width: 100%;
  line-height: 50rpx;
  height: 50rpx;
  margin:auto;
}
.team-cell{
	line-height: 80rpx;
}
.i-modal-main{
	width: 210px;
}