  margin: 0 auto;
}
.member-list{
  padding-top: 50rpx;
  display: flex;
  /* justify-content: space-between; */
  flex-wrap: wrap;
}
.member-item{
  text-align: center;
  margin-bottom: 20rpx;
	width: 33.3333%;
}
.member-item image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%
}
.member-item text{
  display: block;
  color: #666666;
}
