.wrap {
	overflow: hidden;
	min-height: 77vh;
	background: #fff;
}
.searchTeamResult{
	overflow: hidden;
	min-height: 5vh;
	background: #fff;
	margin-bottom: 16rpx;
}
.teamResult .team-head{
	display: flex;
	font-size: 24rpx;
	height: 120rpx;
	align-items: center;
	position: relative;
}
.team-head .head-img{
	width: 90rpx;
	height: 90rpx;
	border-radius: 50%;
}

.teamResult .headInfo{
	margin-left: 26rpx;
	display: flex;
	flex-direction: column;
	align-content: center;
}
.teamResult .headInfo .teamName{
	font-size: 30rpx;
}
.joinTeamStyle{
	position: absolute;
	right: 30rpx;
	top: 30rpx;
	width: 100rpx;
	height: 40rpx;
	border-radius: 6rpx;
	border: 1rpx solid red;
	display: flex;
	font-size: 20rpx;
	 color: red;
	background: none;
	align-items: center;
	justify-content: center;
	padding: 0;
}
.teamSubInfo{
	padding: 0 8rpx;
	box-sizing: border-box;
	display: flex;
	justify-content: space-between;
	font-size: 26rpx;
	align-items: center;
	height: 60rpx;
}
.teamSubInfo .label{
	color: #666
}
.teamDesc{
	padding: 30rpx 6rpx;
	line-height: 46rpx;
	font-size: 28rpx;
}
.btns{
	width: 180rpx;
	height: 50rpx;
	border-radius: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 26rpx;
	margin: 0 auto;
	margin-bottom: 16rpx;
}
.xcx-icon{
	width: 20rpx;
	height: 20rpx;
}
.btns.to-card{
	background: #4e8cee;
	color: #fff;
}
.btns.to-card{
	background: #4e8cee;
	color: #fff;
}
.btns.to-card image{
	margin-right: 6rpx;
}
.btns.out-team{
	background: #f75151;
	color: #fff;
}

page{
	background: #f9f9f9;
}
.topBar{
	height: 54rpx;
	margin: 36rpx auto;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.topBar .searchBar{
	background: #ededed;
	width: 575rpx;
	border-radius: 34rpx;
	padding: 0;
	text-align: center;
	overflow: hidden;
}
.topBar .topMenu {
	font-size: 26rpx;
	display: flex;
	color: #4e8cee;
	align-items: center;
	position: relative;
}
.team-top-title {
	margin-bottom: 10rpx;
}
.team-top-title text{
	font-size: 26rpx;
	padding-left: 8rpx;
}
/* team-list */
.team-item{
	height: 130rpx;
	border-bottom: 1rpx solid #eeeeee;
}
.team-item navigator{
	height: 130rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.team-item .team-item-main{
	display: flex;
	align-items: center;
}
.team-item .team-item-main text{
	margin-left: 26rpx;
	font-size: 26rpx;
}
.team-list .head-img{
	width: 76rpx;
	height: 76rpx;
	border-radius: 50%;
}
.null{
	display: flex;
	justify-content: center;
	align-items: center;
	flex-direction: column;
	min-height: 60vh;
}
.no-msg{
	width: 150rpx;
	height: 150rpx;
}
.null text{
	font-size: 30rpx;
	color: #999;
}