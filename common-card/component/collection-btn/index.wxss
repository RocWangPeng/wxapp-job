.collection-btn{
	width: 64rpx;
	height: 150rpx;
	background: #ffb85c;
	border-radius: 26rpx;
	position: fixed;
	z-index: 200;
	top: 42rpx;
	right: 28rpx;
	box-sizing: border-box;
	padding: 12rpx 0;
	display: flex;
	flex-direction: column;
	justify-content: space-around;
}
.item{
	display: flex;
	justify-content: center;
	align-items: center;
	flex: 1;
}
.item image{
	width: 36rpx;
	height: 36rpx;
}